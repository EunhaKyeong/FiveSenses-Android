<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.NoFeedFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/no_feed_try_cl"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="9dp"
        android:layout_marginBottom="19dp"
        android:background="@drawable/bg_gy01_10"
        app:layout_constraintBottom_toTopOf="@+id/no_feed_how_to_use_btn"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/no_feed_try_tv"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:gravity="center"
            android:includeFontPadding="false"
            android:textAppearance="@style/Head_B_20"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.12" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/no_feed_sight_cl"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="5dp"
            app:layout_constraintBottom_toTopOf="@+id/no_feed_taste_cl"
            app:layout_constraintEnd_toStartOf="@+id/no_feed_ear_cl"
            app:layout_constraintHeight_percent="0.34"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/no_feed_try_tv"
            app:layout_constraintVertical_chainStyle="packed">

            <ImageView
                android:id="@+id/no_feed_sight_iv"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginHorizontal="8dp"
                android:src="@drawable/ic_sight_character_circle_78"
                app:layout_constraintBottom_toTopOf="@+id/no_feed_sight_tv"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/no_feed_sight_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/title_sight"
                android:textAppearance="@style/Sub_Head_B_18"
                android:textColor="@color/RD_3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/no_feed_sight_iv"
                app:layout_constraintStart_toStartOf="@+id/no_feed_sight_iv"
                app:layout_constraintTop_toBottomOf="@+id/no_feed_sight_iv" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/no_feed_ear_cl"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginHorizontal="12dp"
            app:layout_constraintBottom_toTopOf="@+id/no_feed_touch_cl"
            app:layout_constraintEnd_toStartOf="@+id/no_feed_smell_cl"
            app:layout_constraintHeight_percent="0.34"
            app:layout_constraintStart_toEndOf="@+id/no_feed_sight_cl"
            app:layout_constraintTop_toBottomOf="@+id/no_feed_try_tv"
            app:layout_constraintVertical_chainStyle="packed">

            <ImageView
                android:id="@+id/no_feed_ear_iv"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginHorizontal="8dp"
                android:src="@drawable/ic_ear_character_circle_78"
                app:layout_constraintBottom_toTopOf="@+id/no_feed_ear_tv"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/no_feed_ear_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/title_ear"
                android:textAppearance="@style/Sub_Head_B_18"
                android:textColor="@color/BU_3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/no_feed_ear_iv"
                app:layout_constraintStart_toStartOf="@+id/no_feed_ear_iv"
                app:layout_constraintTop_toBottomOf="@+id/no_feed_ear_iv" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/no_feed_smell_cl"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="5dp"
            app:layout_constraintBottom_toTopOf="@+id/no_feed_question_cl"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.34"
            app:layout_constraintStart_toEndOf="@+id/no_feed_ear_cl"
            app:layout_constraintTop_toBottomOf="@+id/no_feed_try_tv"
            app:layout_constraintVertical_chainStyle="packed">

            <ImageView
                android:id="@+id/no_feed_smell_iv"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginHorizontal="8dp"
                android:src="@drawable/ic_smell_character_circle_78"
                app:layout_constraintBottom_toTopOf="@+id/no_feed_smell_tv"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/no_feed_smell_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/title_smell"
                android:textAppearance="@style/Sub_Head_B_18"
                android:textColor="@color/GN_3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/no_feed_smell_iv"
                app:layout_constraintStart_toStartOf="@+id/no_feed_smell_iv"
                app:layout_constraintTop_toBottomOf="@+id/no_feed_smell_iv" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/no_feed_taste_cl"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/no_feed_touch_cl"
            app:layout_constraintHeight_percent="0.34"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/no_feed_sight_cl">

            <ImageView
                android:id="@+id/no_feed_taste_iv"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginHorizontal="8dp"
                android:src="@drawable/ic_taste_character_circle_78"
                app:layout_constraintBottom_toTopOf="@+id/no_feed_taste_tv"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/no_feed_taste_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/title_taste"
                android:textAppearance="@style/Sub_Head_B_18"
                android:textColor="@color/YE_2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/no_feed_taste_iv"
                app:layout_constraintStart_toStartOf="@+id/no_feed_taste_iv"
                app:layout_constraintTop_toBottomOf="@+id/no_feed_taste_iv" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/no_feed_touch_cl"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginHorizontal="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@id/no_feed_ear_cl"
            app:layout_constraintEnd_toStartOf="@+id/no_feed_question_cl"
            app:layout_constraintHeight_percent="0.34"
            app:layout_constraintStart_toEndOf="@+id/no_feed_taste_cl"
            app:layout_constraintTop_toBottomOf="@+id/no_feed_ear_cl">

            <ImageView
                android:id="@+id/no_feed_touch_iv"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginHorizontal="8dp"
                android:src="@drawable/ic_touch_character_circle_78"
                app:layout_constraintBottom_toTopOf="@+id/no_feed_touch_tv"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/no_feed_touch_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/title_touch"
                android:textAppearance="@style/Sub_Head_B_18"
                android:textColor="@color/PU_3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/no_feed_touch_iv"
                app:layout_constraintStart_toStartOf="@+id/no_feed_touch_iv"
                app:layout_constraintTop_toBottomOf="@+id/no_feed_touch_iv" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/no_feed_question_cl"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.34"
            app:layout_constraintStart_toEndOf="@+id/no_feed_touch_cl"
            app:layout_constraintTop_toBottomOf="@+id/no_feed_smell_cl">

            <ImageView
                android:id="@+id/no_feed_question_iv"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginHorizontal="8dp"
                android:src="@drawable/ic_question_character_circle_78"
                app:layout_constraintBottom_toTopOf="@+id/no_feed_question_tv"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/no_feed_question_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/title_question"
                android:textAppearance="@style/Sub_Head_B_18"
                android:textColor="@color/GY_04"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/no_feed_question_iv"
                app:layout_constraintStart_toStartOf="@+id/no_feed_question_iv"
                app:layout_constraintTop_toBottomOf="@+id/no_feed_question_iv" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/no_feed_how_to_use_btn"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginHorizontal="61dp"
        android:layout_marginBottom="26dp"
        android:background="@drawable/bg_bk_22p5"
        android:text="@string/msg_how_to_use"
        android:textAppearance="@style/Sub_Head_B_18"
        android:textColor="@color/WH"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>