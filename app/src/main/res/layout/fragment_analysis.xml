<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <import type="android.view.View" />

        <variable
            name="fragment"
            type="com.mangpo.taste.view.AnalysisFragment" />

        <variable
            name="vm"
            type="com.mangpo.taste.viewmodel.AnalysisViewModel" />

        <variable
            name="userInfo"
            type="com.mangpo.taste.view.model.AnalysisUserInfo" />

        <variable
            name="percentageOfCategory"
            type="java.util.List&lt;com.mangpo.taste.view.model.PercentageOfCategory&gt;" />

        <variable
            name="stat"
            type="com.mangpo.domain.model.getStat.GetStatResEntity" />
    </data>

    <androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/GY_01"
        tools:context=".view.AnalysisFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/analysis_profile_cl"
                android:layout_width="match_parent"
                android:layout_height="397dp"
                android:background="@drawable/bg_wh_bottom30"
                android:paddingHorizontal="20dp"
                android:paddingTop="44dp"
                app:layout_constraintTop_toTopOf="parent">

                <ImageButton
                    android:id="@+id/analysis_setting_ib"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:onClick="@{() -> fragment.toSettingActivity()}"
                    android:src="@drawable/ic_setting_38"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/analysis_nickname_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="31dp"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:text="@{userInfo.nickname}"
                    android:textAppearance="@style/Head_B_26"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/analysis_representative_badge_iv" />

                <ImageButton
                    android:id="@+id/analysis_nickname_edit_ib"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@null"
                    android:onClick="@{() -> fragment.toUpdateNicknameActivity()}"
                    android:src="@drawable/ic_pencil_30"
                    app:layout_constraintBottom_toBottomOf="@+id/analysis_nickname_tv"
                    app:layout_constraintStart_toEndOf="@+id/analysis_nickname_tv"
                    app:layout_constraintTop_toTopOf="@+id/analysis_nickname_tv" />

                <TextView
                    android:id="@+id/analysis_email_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:text="@{userInfo.email}"
                    android:textAppearance="@style/Body1_M_12"
                    android:textColor="@color/GY_03"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/analysis_nickname_tv" />

                <ImageView
                    android:id="@+id/analysis_representative_badge_iv"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_marginTop="13dp"
                    app:defaultDrawable="@{@drawable/ic_no_badge_120}"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/analysis_setting_ib"
                    app:svg="@{vm.representativeBadge.imgUrl}" />

                <ImageView
                    android:id="@+id/analysis_calendar_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:src="@drawable/ic_calendar_14"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/analysis_email_tv" />

                <TextView
                    android:id="@+id/analysis_record_date_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="21dp"
                    android:layout_marginStart="4dp"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:text="@{`기록한지   D+` + userInfo.createdDateCnt}"
                    android:textAppearance="@style/Body1_M_14"
                    android:textColor="@color/GY_04"
                    app:layout_constraintBottom_toBottomOf="@+id/analysis_calendar_iv"
                    app:layout_constraintStart_toEndOf="@+id/analysis_calendar_iv"
                    app:layout_constraintTop_toTopOf="@+id/analysis_calendar_iv" />

                <ImageView
                    android:id="@+id/analysis_locker_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="42dp"
                    android:src="@drawable/ic_locker_14"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/analysis_email_tv" />

                <TextView
                    android:id="@+id/analysis_record_cnt_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="21dp"
                    android:layout_marginStart="4dp"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:text="@{`기록갯수   ` + stat.totalPost + `개`}"
                    android:textAppearance="@style/Body1_M_14"
                    android:textColor="@color/GY_04"
                    app:layout_constraintBottom_toBottomOf="@+id/analysis_locker_iv"
                    app:layout_constraintStart_toEndOf="@+id/analysis_locker_iv"
                    app:layout_constraintTop_toTopOf="@+id/analysis_locker_iv" />

                <View
                    android:id="@+id/analysis_badge_view"
                    android:layout_width="match_parent"
                    android:layout_height="142dp"
                    android:layout_marginBottom="20dp"
                    android:background="@drawable/bg_transparent_12_gy01_2"
                    app:layout_constraintBottom_toBottomOf="parent" />

                <TextView
                    android:id="@+id/analysis_acquired_badge_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="22dp"
                    android:layout_marginTop="17dp"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:text="@string/title_acquired_badge"
                    android:textAppearance="@style/Sub_Head_B_18"
                    app:layout_constraintEnd_toEndOf="@+id/analysis_badge_view"
                    app:layout_constraintStart_toStartOf="@+id/analysis_badge_view"
                    app:layout_constraintTop_toTopOf="@+id/analysis_badge_view" />

                <ImageButton
                    android:id="@+id/analysis_acquired_badge_more_ib"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_marginEnd="2dp"
                    android:background="@null"
                    android:src="@drawable/ic_right_arrow_44"
                    android:onClick="@{() -> fragment.goBadgeActivity(vm.representativeBadge)}"
                    app:layout_constraintEnd_toEndOf="@+id/analysis_badge_view"
                    app:layout_constraintTop_toTopOf="@+id/analysis_badge_view" />

                <ImageView
                    android:id="@+id/analysis_badge1_iv"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="13.5dp"
                    android:layout_marginTop="21dp"
                    android:layout_marginBottom="17dp"
                    app:layout_constraintBottom_toBottomOf="@+id/analysis_badge_view"
                    app:layout_constraintEnd_toStartOf="@+id/analysis_badge2_iv"
                    app:layout_constraintStart_toStartOf="@+id/analysis_badge_view"
                    app:layout_constraintTop_toBottomOf="@+id/analysis_acquired_badge_tv"
                    app:svg="@{vm.acquiredBadges[0].imgUrl}" />

                <ImageView
                    android:id="@+id/analysis_badge2_iv"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="11dp"
                    app:layout_constraintBottom_toBottomOf="@+id/analysis_badge1_iv"
                    app:layout_constraintEnd_toStartOf="@+id/analysis_badge3_iv"
                    app:layout_constraintStart_toEndOf="@+id/analysis_badge1_iv"
                    app:layout_constraintTop_toTopOf="@+id/analysis_badge1_iv"
                    app:svg="@{vm.acquiredBadges[1].imgUrl}" />

                <ImageView
                    android:id="@+id/analysis_badge3_iv"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="11dp"
                    app:layout_constraintBottom_toBottomOf="@+id/analysis_badge1_iv"
                    app:layout_constraintEnd_toStartOf="@+id/analysis_badge4_iv"
                    app:layout_constraintStart_toEndOf="@+id/analysis_badge2_iv"
                    app:layout_constraintTop_toTopOf="@+id/analysis_badge1_iv"
                    app:svg="@{vm.acquiredBadges[2].imgUrl}" />

                <ImageView
                    android:id="@+id/analysis_badge4_iv"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="11dp"
                    android:layout_marginEnd="13.5dp"
                    app:layout_constraintBottom_toBottomOf="@+id/analysis_badge1_iv"
                    app:layout_constraintEnd_toEndOf="@+id/analysis_badge_view"
                    app:layout_constraintStart_toEndOf="@+id/analysis_badge3_iv"
                    app:layout_constraintTop_toTopOf="@+id/analysis_badge1_iv"
                    app:svg="@{vm.acquiredBadges[3].imgUrl}" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.viewpager2.widget.ViewPager2
                android:id="@+id/analysis_banner_vp"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/analysis_profile_cl" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/analysis_distribution_by_sensory_cl"
                android:layout_width="match_parent"
                android:layout_height="205dp"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/bg_wh_12"
                app:layout_constraintTop_toBottomOf="@+id/analysis_banner_vp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
