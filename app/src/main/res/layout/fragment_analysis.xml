                    android:textColor="@{stat.totalPost&lt;10? @color/RD_2: @color/GY_04}"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <!--<com.mangpo.taste.view.custom.RoundedHorizontalBarChart
                    app:rrrr="15dp"
                    android:id="@+id/analysis_stacked_barchart"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/black"
                    android:layout_marginHorizontal="14dp"
                    android:layout_marginTop="76dp"
                    app:layout_constraintTop_toTopOf="parent" />-->

                <com.github.mikephil.charting.charts.HorizontalBarChart
                    android:id="@+id/analysis_stacked_barchart"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_marginHorizontal="14dp"
                    android:layout_marginTop="76dp"
                    android:visibility="@{stat.totalPost&lt;10? View.GONE: View.VISIBLE}"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginHorizontal="10dp"
                    android:layout_marginBottom="71dp"
                    android:background="@color/GY_01"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="37dp"
                    android:layout_marginHorizontal="22dp"
                    android:layout_marginBottom="19dp"
                    android:orientation="horizontal"
                    android:visibility="@{stat.totalPost&lt;10? View.GONE: View.VISIBLE}"
                    app:layout_constraintBottom_toBottomOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/analysis_distribution_cnt_tv1"
                            android:layout_width="wrap_content"
                            android:layout_height="17dp"
                            android:gravity="center"
                            android:includeFontPadding="false"
                            android:text="0"
                            android:textAppearance="@style/Sub_Head_B_14"
                            android:visibility="@{stat.totalPost&lt;10? View.GONE: View.VISIBLE}"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/analysis_distribution_character_iv1"
                            android:layout_width="12dp"
                            android:layout_height="12dp"
                            android:src="@{percentageOfCategory[0].character}"
                            app:layout_constraintBottom_toBottomOf="@+id/analysis_distribution_sense_tv1"
                            app:layout_constraintEnd_toStartOf="@+id/analysis_distribution_sense_tv1"
                            app:layout_constraintHorizontal_chainStyle="packed"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/analysis_distribution_sense_tv1" />

                        <TextView
                            android:id="@+id/analysis_distribution_sense_tv1"
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:gravity="center"
                            android:includeFontPadding="false"
                            android:text="@{percentageOfCategory[0].sense}"
                            android:textAppearance="@style/Body1_M_12"
                            android:textColor="@{percentageOfCategory[0].color}"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/analysis_distribution_character_iv1" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        app:layout_constraintStart_toStartOf="parent">

                        <TextView
                            android:id="@+id/analysis_distribution_cnt_tv2"
                            android:layout_width="wrap_content"
                            android:layout_height="17dp"
                            android:gravity="center"
                            android:includeFontPadding="false"
                            android:text="00"
                            android:textAppearance="@style/Sub_Head_B_14"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/analysis_distribution_character_iv2"
                            android:layout_width="12dp"
                            android:layout_height="12dp"
                            android:src="@{percentageOfCategory[1].character}"
                            app:layout_constraintBottom_toBottomOf="@+id/analysis_distribution_sense_tv2"
                            app:layout_constraintEnd_toStartOf="@+id/analysis_distribution_sense_tv2"
                            app:layout_constraintHorizontal_chainStyle="packed"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/analysis_distribution_sense_tv2" />

                        <TextView
                            android:id="@+id/analysis_distribution_sense_tv2"
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:gravity="center"
                            android:includeFontPadding="false"
                            android:text="@{percentageOfCategory[1].sense}"
                            android:textAppearance="@style/Body1_M_12"
                            android:textColor="@{percentageOfCategory[1].color}"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/analysis_distribution_character_iv2" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        app:layout_constraintStart_toStartOf="parent">

                        <TextView
                            android:id="@+id/analysis_distribution_cnt_tv3"
                            android:layout_width="wrap_content"
                            android:layout_height="17dp"
                            android:gravity="center"
                            android:includeFontPadding="false"
                            android:text="000"
                            android:textAppearance="@style/Sub_Head_B_14"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/analysis_distribution_character_iv3"
                            android:layout_width="12dp"
                            android:layout_height="12dp"
                            android:src="@{percentageOfCategory[2].character}"
                            app:layout_constraintBottom_toBottomOf="@+id/analysis_distribution_sense_tv3"
                            app:layout_constraintEnd_toStartOf="@+id/analysis_distribution_sense_tv3"
                            app:layout_constraintHorizontal_chainStyle="packed"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/analysis_distribution_sense_tv3" />

                        <TextView
                            android:id="@+id/analysis_distribution_sense_tv3"
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:gravity="center"
                            android:includeFontPadding="false"
                            android:text="@{percentageOfCategory[2].sense}"
                            android:textAppearance="@style/Body1_M_12"
                            android:textColor="@{percentageOfCategory[2].color}"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/analysis_distribution_character_iv3" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        app:layout_constraintStart_toStartOf="parent">

                        <TextView
                            android:id="@+id/analysis_distribution_cnt_tv4"
                            android:layout_width="wrap_content"
                            android:layout_height="17dp"
                            android:gravity="center"
                            android:includeFontPadding="false"
                            android:text="0000"
                            android:textAppearance="@style/Sub_Head_B_14"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/analysis_distribution_character_iv4"
                            android:layout_width="12dp"
                            android:layout_height="12dp"
                            android:src="@{percentageOfCategory[3].character}"
                            app:layout_constraintBottom_toBottomOf="@+id/analysis_distribution_sense_tv4"
                            app:layout_constraintEnd_toStartOf="@+id/analysis_distribution_sense_tv4"
                            app:layout_constraintHorizontal_chainStyle="packed"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/analysis_distribution_sense_tv4" />

                        <TextView
                            android:id="@+id/analysis_distribution_sense_tv4"
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:gravity="center"
                            android:includeFontPadding="false"
                            android:text="@{percentageOfCategory[3].sense}"
                            android:textAppearance="@style/Body1_M_12"
                            android:textColor="@{percentageOfCategory[3].color}"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/analysis_distribution_character_iv4" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        app:layout_constraintStart_toStartOf="parent">

                        <TextView
                            android:id="@+id/analysis_distribution_cnt_tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="17dp"
                            android:gravity="center"
                            android:includeFontPadding="false"
                            android:text="00000"
                            android:textAppearance="@style/Sub_Head_B_14"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/analysis_distribution_character_iv5"
                            android:layout_width="12dp"
                            android:layout_height="12dp"
                            android:src="@{percentageOfCategory[4].character}"
