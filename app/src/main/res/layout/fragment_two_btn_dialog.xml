<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".view.TwoBtnDialogFragment">

    <TextView
        android:id="@+id/two_btn_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:textAppearance="@style/Head_B_20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/two_btn_msg_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:textAppearance="@style/Body1_M_16"
        android:textColor="@color/GY_04"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/two_btn_title_tv" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/two_btn_left_btn"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="22dp"
        android:background="@drawable/bg_gy03_22p5"
        android:stateListAnimator="@null"
        android:textAppearance="@style/Sub_Head_B_18"
        android:textColor="@color/WH"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/two_btn_right_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/two_btn_msg_tv" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/two_btn_right_btn"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/bg_gy04_22p5"
        android:stateListAnimator="@null"
        android:textAppearance="@style/Sub_Head_B_18"
        android:textColor="@color/WH"
        app:layout_constraintBottom_toBottomOf="@+id/two_btn_left_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/two_btn_left_btn"
        app:layout_constraintTop_toTopOf="@+id/two_btn_left_btn" />

    <View
        android:id="@+id/two_btn_blurred_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:alpha="0"
        android:visibility="invisible"
        android:background="@drawable/bg_gy01_12"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>